  "devDependencies": {
    "blue-tape": "^0.1.10",
    "es6-promise": "^3.0.2",
    "istanbul": "^0.3.20",
    "nock": "^2.12.0",
    "popsicle": "^1.1.1",
    "pre-commit": "^1.1.1",
    "rimraf": "^2.4.3",
